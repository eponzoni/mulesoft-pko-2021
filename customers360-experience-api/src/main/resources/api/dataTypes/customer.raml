#%RAML 1.0 DataType
usage: Customer personal information.

properties:
  name:
    type: string
    example: 'Harry Potter'
    maxLength: 100
  phone:
    type: string
    example: '+6402175315964'
    maxLength: 50
  email:
    type: string
    example: 'wizard@harrypotter.com'
  born:
    type: date-only
    example: '1981-12-01'
  gender:
    type: string
    enum:
      - M
      - F
    example: 'M'
  sourceSystems:
    type: array
    items:
      type: string
#%RAML 1.0 DataType
usage: Account personal information.

properties:
  firstName:
    type: string
    example: 'Harry'
    maxLength: 50
  lastName:
    type: string
    example: 'Potter'
    maxLength: 50
  phoneNumber:
    type: string
    example: '+6402175315964'
    maxLength: 50
  emailAddress:
    type: string
    example: 'wizard@harrypotter.com'
  birthDate:
    type: date-only
    example: '1981-12-01'
  gender:
    type: string
    enum:
      - M
      - F
    example: 'M'